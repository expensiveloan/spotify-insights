{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/index.tsx",
      "use": "@vercel/next"
    },
    {
      "src": "src/pages/dashboard.tsx",
      "use": "@vercel/next"
    },
    {
      "src": "src/pages/contact.tsx",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/pages/api/$1"
    }
  ]
}